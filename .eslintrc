{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": "off",
        "react/prop-types": "off",
        "no-underscore-dangle": ["error", { "allow": ["_id"] }],
        "comma-dangle": ["error", "only-multiline"],
        "prefer-const": "off",
        "no-plusplus": "off",
        "no-param-reassign": 0,
        "no-return-assign": 0
    },
    env: {
        browser: true
    }
}