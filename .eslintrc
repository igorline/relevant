{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": "off",
        //"react/prop-types": "off",
        "no-underscore-dangle": 0,
        "comma-dangle": ["error", "only-multiline"],
        "prefer-const": "off",
        "no-plusplus": "off",
        "no-param-reassign": 0,
        "no-return-assign": 0,
        "global-require": 0,
        "arrow-parens": 0,
        "prefer-template": 0,
        "no-nested-ternary": 0,
        "no-mixed-operators": 0,
        "no-confusing-arrow": 0,
    },
    env: {
        browser: true
    }
}