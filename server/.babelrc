{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
  ],
  "plugins": [
    "espower",
    "@babel/plugin-transform-runtime",
    "@loadable/babel-plugin",
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {
      "alias": {
        "server": "./server",
        "modules": "./app/modules",
        "core": "./app/core",
        "app": "./app",
        "^react-native$": "react-native-web"
      },
    }]
  ],
}